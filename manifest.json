{
  "manifest_version": 2,
  "name" : "Data Zone",
  "description" : "Creates a zone for our data.",
  "version" : "1.0.0",
  "icons": {
    "48": "icons/border-48.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "contextMenus",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": {
      "48": "icons/icon.png"
    },
    "default_title": "Collected Data"
  }
}
